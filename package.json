{
    "name": "hydro-vault",
    "version": "1.0.0",
    "description": "Hydro Vault Application",
    "engines": {
        "node": "12.12.0"
    },
    "scripts": {
        "check-coverage": "istanbul check-coverage",
        "coverage_": "istanbul cover node_modules/mocha/bin/_mocha -- ./test/index.test.js --opts ./test/mocha.opts",
        "coverage": "nyc mocha ./test/index.test.js --opts ./test/mocha.opts",
        "lint": "eslint src/controllers/*.js test/*.js --fix",
        "fix-lint": "eslint . --fix",
        "test": "yarn run lint && yarn run fix-lint && yarn run coverage && yarn run check-coverage",
        "oldtest": "nyc --reporter=html mocha ./test/index.test.js --opts ./test/mocha.opts",
        "newtest_": "yarn run coverage && yarn run check-coverage",
        "newtest": "yarn run coverage",
        "startdev": "nodemon ./bin/www",
        "start": "node ./bin/www",
        "check-libs": "snyk test --dev",
        "new": "mocha ./test/index.test.js --opts ./test/mocha.opts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HydroBlockchain/vault"
    },
    "author": "Oluwafemi Akinde",
    "bugs": {
        "url": "https://github.com/HydroBlockchain/vault/issues"
    },
    "homepage": "https://github.com/HydroBlockchain/vault#readme",
    "main": "server.js",
    "license": "MIT",
    "dependencies": {
        "@bugsnag/js": "^6.4.1",
        "@bugsnag/plugin-express": "^6.4.1",
        "@hydrogenplatform/raindrop": "^0.2.7",
