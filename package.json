        "agenda": "^2.1.0",
        "ajv": "^6.10.2",
        "ajv-errors": "^1.0.1",
        "app-root-path": "^2.2.1",
        "await-of": "^1.2.0",
        "axios": "^0.19.0",
        "bcrypt": "^3.0.6",
        "bignumber.js": "^9.0.0",
        "boom": "^7.3.0",
        "check-dependencies": "^1.1.0",
        "coingecko-api": "^1.0.6",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cron": "^1.7.2",
        "crypto-js": "^3.1.9-1",
        "crypto-random-string": "^3.0.1",
        "dotenv": "^8.1.0",
